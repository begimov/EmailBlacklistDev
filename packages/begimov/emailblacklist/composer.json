{
    "name": "begimov/emailblacklist",
    "description": "Package to create and maintain email blacklist",
    "license": "MIT",
    "authors": [
        {
            "name": "Galymzhan Begimov",
            "email": "begimov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Begimov\\Emailblacklist\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "illuminate/database": ">=5.0",
        "illuminate/support": ">=5.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7",
        "mockery/mockery": "^1.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/stubs/UserStub.php",
            "tests/stubs/EmailStub.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Begimov\\Emailblacklist\\EmailBlacklistServiceProvider"
            ]
        }
    }
}
